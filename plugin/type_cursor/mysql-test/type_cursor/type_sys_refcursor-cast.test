--echo #
--echo # Type cast to other data types
--echo #

DELIMITER $$;
--error ER_ILLEGAL_PARAMETER_DATA_TYPE_FOR_OPERATION
BEGIN NOT ATOMIC
  DECLARE c0 SYS_REFCURSOR;
  SELECT CAST(c0 AS SIGNED);
END;
$$
DELIMITER ;$$

